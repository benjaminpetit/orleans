<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Microsoft.Orleans.GrainDirectory.Redis</PackageId>
    <Title>Microsoft Orleans Grain Directory Redis</Title>
    <Description>Microsoft Orleans Grain Directory implementation that uses Refis</Description>
    <PackageTags>$(PackageTags) Redis Grain Directory</PackageTags>
    <TargetFrameworks>$(StandardTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" Version="$(StackExchangeRedis)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Orleans.Core.Abstractions\Orleans.Core.Abstractions.csproj" />
    <ProjectReference Include="..\Orleans.Runtime.Abstractions\Orleans.Runtime.Abstractions.csproj" />
    <ProjectReference Include="..\OrleansProviders\OrleansProviders.csproj" />
  </ItemGroup>

</Project>
